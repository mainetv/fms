{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-gd": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laracasts/utilities": "^3.2",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.7",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.5",
        "owen-it/laravel-auditing": "^13.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.15",
        "mockery/mockery": "^1.4.4",
        "nascent-africa/jetstrap": "^2.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/functions_dashboard.php",
            "app/Http/functions_proposals.php",
            "app/Http/functions_nep.php",
            "app/Http/functions_master.php",
            "app/Http/functions_bp_forms.php",
            "app/Http/functions_physical_targets.php",
            "app/Http/functions_allotment.php",
            "app/Http/functions_rs.php",
            "app/Http/functions_dv.php",
            "app/Http/functions_lddap_ada.php",
            "app/Http/functions_reports.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
